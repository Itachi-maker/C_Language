// Multiplication table by skipping 1 and 10 multiples upto 16
#include <stdio.h>
int main() {
    int n, i;
    printf("Enter the number:");
    scanf("%d", &n);
    if(n==10 || n==1){
        printf("Cannot Accept");
    }else{
    printf("\nMultiplication Table of %d:", n);
    for(i=2; i<=16; i++){
    if(i==10)
    continue;
        printf("\n %d x %d = %d", i, n, i*n);
    }
}
    return 0;
}
