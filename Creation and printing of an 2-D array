//Creation and printing of an 2-D array
#include<stdio.h>
int main(){ 
    int r, c;
    printf("Enter the dimensions(row and column number) of the 2d array:");
    scanf("%d%d", &r, &c); 
    int a[r][c];
    printf("Enter %d elements\n: ", r*c);
    for(int i=0; i<r; i++){
        for(int j=0; j<c; j++){
            scanf("%d", &a[i][j]);
        }
    }
    for(int i=0; i<r; i++){
        for(int j=0; j<c; j++){
            printf("%d ", a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
