// Display the Factors of a number
#include <stdio.h>
void main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    printf("Factors of %d are: ", n);
    for (int i = 2; i <= n/2; i++) {
        if (n % i == 0) {
            printf("\n%d ", i);
        }
    }
}
//Output:
Enter a number: 20
Factors of 20 are: 
2 
4 
5 
10 
