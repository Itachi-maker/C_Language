// Sum of elements in an Array
#include <stdio.h>
int main() {
    int size;
    printf("Enter the size of the Array:");
    scanf("%d", size);
    int a[size];
    for(int i =0; i < size ; i++)
        scanf("%d", a[i]);
    int sum=0;
    for(int i =0; i < size ; i++)
        sum+=a[i];
    printf("%d", sum);
    return 0;
    }
